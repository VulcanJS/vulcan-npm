    "@vulcanjs/permissions": "^0.7.0",
    "@vulcanjs/schema": "^0.7.0",
    "@vulcanjs/utils": "^0.7.0",
    "apollo-datasource": "^3.3.1",
    "debug": "^4.3.1",
    "deepmerge": "^4.2.2",
